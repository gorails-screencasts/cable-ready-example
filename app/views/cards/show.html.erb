<div class="page-header">
  <%= link_to cards_path, class: 'btn btn-default' do %>
    All Cards
  <% end %>
  <%= link_to edit_card_path(@card), class: 'btn btn-primary' do %>
    Edit
  <% end %>
  <h1>Show card</h1>
</div>

<dl class="dl-horizontal">
  <dt>Title:</dt>
  <dd><%= @card.title %></dd>

  <dt>Description:</dt>
  <dd><%= @card.description %></dd>

</dl>
